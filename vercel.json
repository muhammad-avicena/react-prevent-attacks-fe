{
    "rewrites": [
      {"source": "/(.*)", "destination": "/"}
    ],

        "headers": [
          {
            "source": "/(.*)",
            "headers": [
              {
                "key": "Content-Security-Policy",
                "value": "frame-ancestors 'self' https://week15.avicena.dev;"
              }
            ]
          }
        ]
  }
  